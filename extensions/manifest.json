{
  "manifest_version": 3,
  "minimum_chrome_version": "100",
  "name" : "Spector.js",
  "version" : "0.9.32",
  "homepage_url": "http://spector.babylonjs.com/",
  "description" : "Explore and Troubleshoot your WebGL scenes easily.",
  "author": "@SpectorJS",
  "host_permissions": [
    "http://*/*", "https://*/*"
  ],
  "permissions": ["storage", "unlimitedStorage", "activeTab", "scripting"],
  "action" :
  {
    "default_icon" : {
      "19": "spectorjs-19.png",
      "38": "spectorjs-38.png"
    },
    "default_title" : "Debug with Spector"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts" : [
    {
      "matches" : [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js" : ["contentScriptProxy.js"],
      "run_at" : "document_start",
      "all_frames" : true,
      "world": "ISOLATED"
    },
    {
      "matches" : [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js" : ["spector.bundle.func.js", "contentScript.js"],
      "run_at" : "document_start",
      "all_frames" : true,
      "world": "MAIN"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "spector.bundle.js",
        "popup.js",
        "result.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "icons" : {
    "48" : "spectorjs-48.png",
    "128" : "spectorjs-128128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
   }
}
